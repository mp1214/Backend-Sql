-- -----------------------------------------------------\par
-- Table `portfolio`.`persona`\par
-- -----------------------------------------------------\par
CREATE TABLE IF NOT EXISTS `portfolio`.`persona` (\par
  `id` INT UNSIGNED NOT NULL AUTO_INCREMENT,\par
  `nombre` VARCHAR(45) NOT NULL,\par
  `apellido` VARCHAR(45) NOT NULL,\par
  `telefono` INT NOT NULL,\par
  `sobremi` LONGTEXT NOT NULL,\par
  `provincia` VARCHAR(45) NOT NULL,\par
  `img` VARCHAR(100) NOT NULL,\par
  `banner` VARCHAR(45) NOT NULL,\par
  `e-mail` VARCHAR(45) NOT NULL,\par
  `fortaleza` VARCHAR(45) NULL,\par
  `debilidad` VARCHAR(45) NULL,\par
  `estcivil` VARCHAR(45) NULL,\par
  PRIMARY KEY (`id`))\par
ENGINE = InnoDB;\par
\par
CREATE UNIQUE INDEX `e-mail_UNIQUE` ON `portfolio`.`persona` (`e-mail` ASC) VISIBLE;\par
\par
\par
-- -----------------------------------------------------\par
-- Table `portfolio`.`experiencia`\par
-- -----------------------------------------------------\par
CREATE TABLE IF NOT EXISTS `portfolio`.`experiencia` (\par
  `id` INT NOT NULL AUTO_INCREMENT,\par
  `cargo` VARCHAR(45) NOT NULL,\par
  `inicio` DATE NOT NULL,\par
  `fin` VARCHAR(45) NULL,\par
  `descripcion` LONGTEXT NOT NULL,\par
  `img` VARCHAR(45) NULL,\par
  `empresa` VARCHAR(45) NOT NULL,\par
  `persona_id` INT UNSIGNED NOT NULL,\par
  `estrabajoactual` TINYINT NULL,\par
  PRIMARY KEY (`id`))\par
ENGINE = InnoDB;\par
\par
\par
-- -----------------------------------------------------\par
-- Table `portfolio`.`certificado`\par
-- -----------------------------------------------------\par
CREATE TABLE IF NOT EXISTS `portfolio`.`certificado` (\par
  `id` INT NOT NULL,\par
  `titulo` VARCHAR(45) NOT NULL,\par
  `img` VARCHAR(45) NULL,\par
  PRIMARY KEY (`id`))\par
ENGINE = InnoDB;\par
\par
\par
-- -----------------------------------------------------\par
-- Table `portfolio`.`estudio`\par
-- -----------------------------------------------------\par
CREATE TABLE IF NOT EXISTS `portfolio`.`estudio` (\par
  `id` INT NOT NULL,\par
  `titulo` VARCHAR(45) NOT NULL,\par
  `inicio` DATE NOT NULL,\par
  `fin` DATE NULL,\par
  `institucion` VARCHAR(45) NOT NULL,\par
  `img` VARCHAR(45) NULL,\par
  `curso` ENUM('formal', 'curso', 'bootcamp') NOT NULL,\par
  `persona_id` INT UNSIGNED NOT NULL,\par
  `certificado_id` INT NOT NULL,\par
